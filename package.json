{
  "name": "sazare-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "spell-check": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky && panda codegen"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@eslint/js": "^9.25.0",
    "@pandacss/dev": "^0.53.7",
    "@storybook/addon-a11y": "^9.0.1",
    "@storybook/addon-docs": "^9.0.1",
    "@storybook/addon-onboarding": "^9.0.1",
    "@storybook/addon-vitest": "^9.0.1",
    "@storybook/react-vite": "^9.0.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "cspell": "^9.2.1",
    "eslint": "^9.25.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "playwright": "^1.52.0",
    "prettier": "^3.6.2",
    "storybook": "^9.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "cspell --cache --no-summary --no-must-find-files"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
